@model BloodDonor.Web.ViewModels.Requests.RequestViewModel

<h1>@Model.PatientFullName needs your help!</h1>

<hr />

<link href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css" rel="stylesheet">
<div class="container">
    <div class="row">
        <div class="col-md-offset-2 col-md-8 col-lg-offset-3 col-lg-6">
            <div class="well profile">
                <div class="col-sm-12">
                    <img src="data:image/jpeg;base64," alt="" class="img-circle img-responsive">
                    <div class="col-xs-12 col-sm-8">
                        <h2>@Model.PatientFullName</h2>
                        <p><strong>Medical Condition:  </strong>@Model.MedicalCondition</p>
                        <p><strong>Blood Type: </strong>@Model.PatientBloodType</p>
                        <p><strong>Blood banks needed: </strong>@Model.Quantity</p>
                        <p><strong>Location: </strong>@Model.PatientLocationTownName</p>
                        <p><strong>Personnal Message: </strong> @Model.PeronalMessage </p>
                    </div>
                    
                </div>
                      <a class="btn btn-danger btn-block" asp-controller="Responses" asp-action="Add">Response to this Request</a>
                </div>
            </div>
        </div>
    </div>
</div>


